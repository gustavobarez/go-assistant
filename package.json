{
  "name": "go-assistant",
  "displayName": "Go Assistant",
  "description": "VS Code extension for Go that displays references and methods for structs, interfaces, and functions.",
  "icon": "icon.png",
  "publisher": "gustavobarez",
  "author": {
    "name": "Gustavo Barez"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gustavobarez/go-assistant"
  },
  "version": "0.1.1",
  "engines": {
    "vscode": "^1.109.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:go"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "go-assistant.showReferences",
        "title": "Go Assistant: Show References"
      },
      {
        "command": "go-assistant.showImplementations",
        "title": "Go Assistant: Show Implementations"
      },
      {
        "command": "go-assistant.showImplementers",
        "title": "Go Assistant: Show Implementers"
      },
      {
        "command": "go-assistant.addImport",
        "title": "Go Assistant: Add Import"
      },
      {
        "command": "go-assistant.clearReferencesView",
        "title": "Clear",
        "icon": "$(clear-all)"
      },
      {
        "command": "go-assistant.refreshReferencesView",
        "title": "Refresh",
        "icon": "$(refresh)"
      },
      {
        "command": "go-assistant.discoverTests",
        "title": "Discover Tests",
        "icon": "$(refresh)"
      },
      {
        "command": "go-assistant.runAllTests",
        "title": "Run All Tests",
        "icon": "$(run-all)"
      },
      {
        "command": "go-assistant.runAllTestsWithCoverage",
        "title": "Run All Tests with Shared Coverage",
        "icon": "$(run-all)"
      },
      {
        "command": "go-assistant.runPackageTests",
        "title": "Run Package Tests",
        "icon": "$(run)"
      },
      {
        "command": "go-assistant.runFileTests",
        "title": "Run File Tests",
        "icon": "$(run)"
      },
      {
        "command": "go-assistant.runTest",
        "title": "Run Test",
        "icon": "$(run)"
      },
      {
        "command": "go-assistant.debugPackageTests",
        "title": "Debug Package Tests",
        "icon": "$(debug)"
      },
      {
        "command": "go-assistant.debugFileTests",
        "title": "Debug File Tests",
        "icon": "$(debug)"
      },
      {
        "command": "go-assistant.debugTest",
        "title": "Debug Test",
        "icon": "$(debug)"
      },
      {
        "command": "go-assistant.openCoverageHtml",
        "title": "Open Coverage HTML",
        "icon": "$(open-preview)"
      },
      {
        "command": "go-assistant.clearCoverage",
        "title": "Clear Coverage Decorations",
        "icon": "$(clear-all)"
      }
    ],
    "views": {
      "go-assistant-references": [
        {
          "id": "goAssistantReferences",
          "name": "References (Go Assistant)",
          "when": "goAssistant.hasReferences"
        }
      ],
      "go-assistant-tests": [
        {
          "id": "goAssistantTests",
          "name": "Tests (Go Assistant)"
        }
      ]
    },
    "viewsContainers": {
      "panel": [
        {
          "id": "go-assistant-references",
          "title": "References (Go Assistant)",
          "icon": "$(references)"
        },
        {
          "id": "go-assistant-tests",
          "title": "Tests (Go Assistant)",
          "icon": "$(beaker)"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "go-assistant.refreshReferencesView",
          "when": "view == goAssistantReferences",
          "group": "navigation"
        },
        {
          "command": "go-assistant.clearReferencesView",
          "when": "view == goAssistantReferences",
          "group": "navigation"
        },
        {
          "command": "go-assistant.discoverTests",
          "when": "view == goAssistantTests",
          "group": "navigation"
        },
        {
          "submenu": "go-assistant.testsMenu",
          "when": "view == goAssistantTests",
          "group": "navigation@1"
        }
      ],
      "view/item/context": [
        {
          "command": "go-assistant.runPackageTests",
          "when": "view == goAssistantTests && viewItem == package",
          "group": "inline@1"
        },
        {
          "command": "go-assistant.debugPackageTests",
          "when": "view == goAssistantTests && viewItem == package",
          "group": "inline@2"
        },
        {
          "command": "go-assistant.runFileTests",
          "when": "view == goAssistantTests && viewItem == file",
          "group": "inline@1"
        },
        {
          "command": "go-assistant.debugFileTests",
          "when": "view == goAssistantTests && viewItem == file",
          "group": "inline@2"
        },
        {
          "command": "go-assistant.runTest",
          "when": "view == goAssistantTests && viewItem == test",
          "group": "inline@1"
        },
        {
          "command": "go-assistant.debugTest",
          "when": "view == goAssistantTests && viewItem == test",
          "group": "inline@2"
        }
      ]
    },
    "submenus": [
      {
        "id": "go-assistant.testsMenu",
        "label": "Run All Tests",
        "icon": "$(run-all)"
      }
    ],
    "configuration": {
      "title": "Go Assistant",
      "properties": {
        "goAssistant.codelens.enable": {
          "type": "boolean",
          "default": true,
          "description": "Habilita/desabilita todos os CodeLens"
        },
        "goAssistant.codelens.references": {
          "type": "boolean",
          "default": true,
          "description": "Mostra contagem de referências para funções, métodos, structs e interfaces"
        },
        "goAssistant.codelens.methods": {
          "type": "boolean",
          "default": true,
          "description": "Mostra contagem de métodos para structs"
        },
        "goAssistant.codelens.implementers": {
          "type": "boolean",
          "default": true,
          "description": "Mostra contagem de implementadores para interfaces"
        },
        "goAssistant.codelens.implementations": {
          "type": "boolean",
          "default": true,
          "description": "Mostra contagem de interfaces implementadas por tipos"
        },
        "goAssistant.codelens.fields": {
          "type": "boolean",
          "default": false,
          "description": "Mostra contagem de referências para campos de structs (pode deixar lento em projetos grandes)"
        },
        "goAssistant.codelens.packageImports": {
          "type": "boolean",
          "default": true,
          "description": "Mostra quantos arquivos importam este pacote"
        },
        "goAssistant.codelens.largeProject": {
          "type": "boolean",
          "default": false,
          "description": "Habilita otimizações para projetos grandes (pode consumir mais RAM e deixar startup mais lento, mas melhora performance de referências)"
        },
        "goAssistant.codelens.runDebug": {
          "type": "boolean",
          "default": true,
          "description": "Mostra botões Run/Debug para pacotes main"
        },
        "goAssistant.codelens.debugTests": {
          "type": "boolean",
          "default": true,
          "description": "Mostra botões Run/Debug para table-driven tests"
        },
        "goAssistant.inlayHints.enable": {
          "type": "boolean",
          "default": false,
          "description": "Mostra contagem de referências como inlay hints inline"
        },
        "goAssistant.inlayHints.buildTags": {
          "type": "boolean",
          "default": true,
          "description": "Mostra diagnóstico de build tags (se o arquivo será compilado)"
        },
        "goAssistant.postfixCompletion.enable": {
          "type": "boolean",
          "default": true,
          "description": "Habilita postfix completions (.if, .for, .print, etc.)"
        },
        "goAssistant.codeActions.enable": {
          "type": "boolean",
          "default": true,
          "description": "Habilita code actions (refatorações rápidas)"
        },
        "goAssistant.codeActions.handleError": {
          "type": "boolean",
          "default": true,
          "description": "Mostra ações rápidas para tratar erros"
        },
        "goAssistant.codeActions.extractVariable": {
          "type": "boolean",
          "default": true,
          "description": "Mostra ação de extrair variável"
        },
        "goAssistant.codeActions.showReferences": {
          "type": "boolean",
          "default": true,
          "description": "Mostra ação de visualizar referências"
        },
        "goAssistant.codeActions.showImplementations": {
          "type": "boolean",
          "default": true,
          "description": "Mostra ações de visualizar implementações/implementadores"
        },
        "goAssistant.inspections.enable": {
          "type": "boolean",
          "default": true,
          "description": "Habilita inspeções/diagnósticos de código"
        },
        "goAssistant.inspections.unusedAssignments": {
          "type": "boolean",
          "default": true,
          "description": "Detecta atribuições não utilizadas"
        },
        "goAssistant.inspections.unusedSymbols": {
          "type": "boolean",
          "default": false,
          "description": "Detecta símbolos não utilizados (desabilitado por padrão, gopls já faz isso)"
        },
        "goAssistant.inspections.unreachableCode": {
          "type": "boolean",
          "default": true,
          "description": "Detecta código inacessível após return/panic"
        },
        "goAssistant.inspections.unhandledErrors": {
          "type": "boolean",
          "default": true,
          "description": "Detecta erros não tratados"
        },
        "goAssistant.inspections.shadowedVariables": {
          "type": "boolean",
          "default": true,
          "description": "Detecta variáveis sombreadas"
        },
        "goAssistant.inspections.deprecatedSymbols": {
          "type": "boolean",
          "default": false,
          "description": "Detecta símbolos deprecados (apenas se vscode-go não estiver instalado)"
        },
        "goAssistant.debugging.inlineValues": {
          "type": "boolean",
          "default": true,
          "description": "Mostra valores inline durante debugging"
        },
        "goAssistant.helpers.autoUpdateImports": {
          "type": "boolean",
          "default": true,
          "description": "Atualiza automaticamente imports quando arquivos são movidos"
        },
        "goAssistant.helpers.autoUpdateReferences": {
          "type": "boolean",
          "default": true,
          "description": "Atualiza automaticamente referências quando arquivos são movidos"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "pnpm run package",
    "compile": "pnpm run check-types && pnpm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "pnpm run check-types && pnpm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.109.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.54.0",
    "eslint": "^9.39.2",
    "esbuild": "^0.27.2",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  },
  "packageManager": "pnpm@10.29.3+sha512.498e1fb4cca5aa06c1dcf2611e6fafc50972ffe7189998c409e90de74566444298ffe43e6cd2acdc775ba1aa7cc5e092a8b7054c811ba8c5770f84693d33d2dc"
}
